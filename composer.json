{
  "name": "octahedron/pulp",
  "description": "A PHP dependency injection framework based on Google's Guice",
  "type": "library",
  "keywords": [
    "dependency",
    "injection",
    "guice"
  ],
  "homepage": "https://github.com/octahedron/pulp",
  "license": "MIT",
  "authors": [{"name": "Andy Shea", "email": "andrew@octahedron.com.au"}],
  "require": {
    "php": ">=5.5",
    "doctrine/annotations": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mikey179/vfsStream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Octahedron\\Pulp\\": "lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Octahedron\\Pulp\\Test\\": "test"
    }
  }
}
